<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>
<FlowPane alignment="CENTER_LEFT" columnHalignment="CENTER" hgap="30.0" vgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <FlowPane alignment="CENTER_LEFT" columnHalignment="CENTER" hgap="10.0" prefWrapLength="200.0" rowValignment="BASELINE">
         <children>
            <TextField fx:id="SearchField" promptText="Search..." />
         </children>
      </FlowPane>
      <CheckBox fx:id="MatchCase" mnemonicParsing="false" text="Match Case" />
      <Button fx:id="Next" mnemonicParsing="false" text="&gt;&gt;" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</FlowPane>
